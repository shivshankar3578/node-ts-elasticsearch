    "license": "MIT",
    "description": "Elasticsearch decorated by TypeScript",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gojob-1337/node-ts-elasticsearch"
